    "rimraf": "^3.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "rollup": "npx rollup -c rollup.config.js --bundleConfigAsCjs",
