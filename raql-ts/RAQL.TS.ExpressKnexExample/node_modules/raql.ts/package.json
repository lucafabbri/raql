{
  "name": "raql.ts",
  "version": "1.0.0",
  "description": "Rest API Query Language",
  "main": "out/plugin.cjs.js",
  "module": "out/index.js",
  "types": "out/index.d.ts",
  "unpkg": "out/plugin.js",
  "dependencies": {
    "antlr4": "^4.11.0",
    "antlr4ts": "^0.5.0-alpha.4",
    "knex": "^2.3.0"
  },
  "devDependencies": {
    "@types/antlr4": "^4.11.1",
